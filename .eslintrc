{
    "env": {
        "browser": true,
        "node": true,
        "es2020": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "no-empty": 0,
        "no-redeclare": 0,
        "no-useless-escape": 0,
        "no-inner-declarations": 0,
        "object-shorthand": ["error", "properties"],
        "prefer-template": "error",
        "no-useless-concat": "error"
    }
}
